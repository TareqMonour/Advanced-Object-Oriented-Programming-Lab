<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo2.TableViewExampleController" spacing="10" alignment="TOP_CENTER">
    <TableView fx:id="tableView" prefHeight="300" prefWidth="400">
        <columns>
            <TableColumn fx:id="nameColumn" text="Name" />
            <TableColumn fx:id="ageColumn" text="Age" />
        </columns>
    </TableView>
    <Button text="Add Row" onAction="#handleAddRow" />
</VBox>
